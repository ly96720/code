.item {
  width: 100%;
  height: 96rpx;
  background-color: #fff;
  line-height: 96rpx;
  font-size: 28rpx;
  padding-left: 40rpx;
  color: #191a1b;
  border-bottom: 2rpx solid #eee;
}

.selectCity {
  padding-left: 30rpx;
  width: 100%;
  height: 380rpx;
  background-color: white;
}

.GPS {
  margin-top: 15rpx;
  font-size: 26rpx;
  color: #797d82;
  margin-bottom: 20rpx;
  text-align: left;
  line-height: 50rpx;
}

.fail {
  height: 60rpx;
  background-color: #f4f4f4;
  line-height: 60rpx;
  border-radius: 6rpx;
  -webkit-box-sizing: border-box;
  box-sizing: border-box;
  margin: 0 15rpx;
  width: 180rpx;
  font-size: 28rpx;
  color: #191a1b;
  text-align: center;
}

.ul {
  display: grid;
  grid-template-columns: 33% 33% 33%;
}

.li {
  height: 60rpx;
  background-color: #f4f4f4;
  line-height: 60rpx;
  border-radius:15rpx;
  -webkit-box-sizing: border-box;
  box-sizing: border-box;
  margin: 0 10rpx;
  font-size: 28rpx;
  color: #191a1b;
  width: 180rpx;
  text-align: center;
  margin-bottom:30rpx;
}

.content {
  margin: 5rpx 0 0 0;
  overflow-y: auto;
  background-color: #fff;
}


.van-search--withaction{
  margin-top: 5rpx!important;
}